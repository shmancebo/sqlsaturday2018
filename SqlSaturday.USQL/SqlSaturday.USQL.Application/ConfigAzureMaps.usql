REFERENCE ASSEMBLY ImageCommon;

@imgsTags =
    EXTRACT 
        FileName string, 
        ImgData byte[]
    FROM @"/csImages/Vision%20Api/{FileName}.jpg"
    USING new Cognition.Vision.ImageExtractor();

@tags=
    SELECT FileName
    FROM @imgsTags;

OUTPUT @tags
     TO "AzureMaps/filesProcess.csv"
     USING Outputters.Csv(rowDelimiter: "\u0003");

